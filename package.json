{
  "name": "api",
  "version": "1.0.0",
  "main": "./dist/server.js",
  "scripts": {
    "test": "",
    "dev": "nodemon ./src/server.ts",
    "build": "rimraf ./dist && tsc",
    "start": "node ./dist/server.js",
    "watch": "tsc --watch",
    "knexInit": "knex init",
    "migrate": "cd src/config/db && knex migrate:make",
    "runMigrate:dev": "knex migrate:latest --knexfile ./src/config/db/knexfile.ts --env=development",
    "runMigrate:prod": "knex migrate:latest --knexfile ./src/config/db/knexfile.ts --env=production",
    "runRollback": "knex migrate:rollback --knexfile ./src/config/db/knexfile.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "lendsr assessment backend test.",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^2.5.1",
    "mysql": "^2.18.1",
    "mysql2": "^3.6.2",
    "short-uuid": "^4.2.2",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.8.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.14",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/node": "^18.11.10",
    "dotenv": "^16.0.3",
    "nodemon": "^2.0.22",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3"
  }
}
